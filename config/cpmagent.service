[Unit]
Description=cpmagent
After=network.target

[Service]
Type=forking
TimeoutSec=120
User=root
Group=root

Environment="PATH=/cluster/bin:/usr/pgsql-9.3/bin:/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin:/root/bin"
SyslogIdentifier=cpmagent


ExecStart=/cluster/bin/start-cpmagent.sh
ExecReload=/cluster/bin/reload-cpmagent.sh
ExecStop=killall cpmagentserver

[Install]
WantedBy=multi-user.target

